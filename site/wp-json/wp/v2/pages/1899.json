r: red;\n  font-size: 12px;\n  display: none;\n}\n\n\/* Button Styling *\/\n.button-container {\n  text-align: center;\n  margin-top: 40px;\n}\n\n.wpcf7 input[type=\"submit\"] {\n  background-color: #2bb492;\n  color: #fff;\n  font-size: 16px;\n  height: 50px;\n  border: none;\n  cursor: pointer;\n  border-radius: 5px;\n  line-height: 50px;\n  display: inline-block;\n  width: 100%;\n  padding: 0px;\n}\n\n.wpcf7 input[type=\"submit\"]:hover {\n  background-color: #229e7d;\n}\n<\/style>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n    \/\/ Handle form submission via Contact Form 7 event\n    document.addEventListener('wpcf7mailsent', function(event) {\n        \/\/ Retrieve form data from event\n        const formData = event.detail.inputs;\n        \n        \/\/ Prepare data for localStorage and redirection\n        const formDetails = {};\n        formData.forEach(input => {\n            formDetails[input.name] = input.value;\n        });\n\n        \/\/ Save to localStorage with more reliable keys\n        Object.keys(formDetails).forEach(key => {\n            localStorage.setItem(`cf7_${key}`, formDetails[key]);\n        });\n\n        \/\/ Construct query parameters\n        const queryParams = new URLSearchParams();\n        Object.keys(formDetails).forEach(key => {\n            queryParams.append(key, formDetails[key]);\n        });\n\n        \/\/ Redirect with query parameters\n        window.location.href = `https:\/\/dubailegalcheck.ae\/police-clearance-checking\/?${queryParams.toString()}`;\n    }, false);\n\n    \/\/ On the next page, retrieve and display data\n    if (window.location.pathname.includes('\/police-clearance-checking\/')) {\n        \/\/ Retrieve data from query parameters first\n        const urlParams = new URLSearchParams(window.location.search);\n        const storedData = {};\n\n        \/\/ Try to get data from URL parameters\n        urlParams.forEach((value, key) => {\n            storedData[key] = value;\n        });\n\n        \/\/ If URL params are empty, try localStorage\n        if (Object.keys(storedData).length === 0) {\n            \/\/ Retrieve from localStorage with CF7 prefix\n            const localStorageData = [\n                'full-name', 'emirates-id', 'email', \n                'mobile-number', 'countrytext-427'\n            ];\n\n            localStorageData.forEach(key => {\n                const storedValue = localStorage.getItem(`cf7_${key}`);\n                if (storedValue) {\n                    storedData[key] = storedValue;\n                }\n            });\n        }\n\n        \/\/ Display data (customize selectors as needed)\n        Object.keys(storedData).forEach(key => {\n            const displayElement = document.querySelector(`[data-form-field=\"${key}\"]`);\n            if (displayElement) {\n                displayElement.textContent = storedData[key];\n                displayElement.value = storedData[key];\n            }\n            console.log(`${key}: ${storedData[key]}`);\n        });\n\n        \/\/ Optional: Clear localStorage after retrieving\n        localStorage.clear();\n    }\n});\n\n\n\n\n\n\n\ndocument.addEventListener('DOMContentLoaded', function () {\n    document.addEventListener('submit', function (event) {\n        \/\/ Check if the form being submitted is a Contact Form 7 form\n        if (event.target.classList.contains('wpcf7-form')) {\n            \/\/ Get Emirates ID and Mobile Number values\n            let emiratesIdValue = document.getElementById('emirates-id-input').value;\n            let mobileNumberValue = document.getElementById('mobile-number').value;\n\n            \/\/ Set values in hidden fields\n            document.querySelector('[name=\"emirates-id-field\"]').value = emiratesIdValue;\n            document.querySelector('[name=\"mobile-number-field\"]').value = mobileNumberValue;\n\n            \/\/ Debugging check (View values in console)\n            console.log(\"Emirates ID:\", emiratesIdValue);\n            console.log(\"Mobile Number:\", mobileNumberValue);\n        }\n    }, true); \/\/ Use 'true' to ensure it fires before CF7 processes the form\n});\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\ndocument.addEventListener('DOMContentLoaded', function() {\n    document.addEventListener('wpcf7mailsent', function(event) {\n        const formData = event.detail.inputs;\n        const formDetails = {};\n        formData.forEach(input => {\n            formDetails[input.name] = input.value;\n        });\n\n        \/\/ Include manually added values\n        formDetails[\"emirates-id\"] = document.getElementById(\"emirates-id-input\").value;\n        formDetails[\"mobile-number\"] = document.getElementById(\"mobile-number\").value;\n\n        localStorage.setItem(\"cf7_emirates-id\", formDetails[\"emirates-id\"]);\n        localStorage.setItem(\"cf7_mobile-number\", formDetails[\"mobile-number\"]);\n\n        const queryParams = new URLSearchParams(formDetails).toString();\n        window.location.href = `https:\/\/dubailegalcheck.ae\/police-clearance-checking\/?${queryParams}`;\n    }, false);\n});\ndocument.addEventListener('DOMContentLoaded', function () {\n    document.addEventListener('submit', function (event) {\n        if (event.target.classList.contains('wpcf7-form')) {\n            \/\/ Get Emirates ID and Mobile Number values\n            let emiratesIdValue = document.getElementById('emirates-id-input').value;\n            let mobileNumberValue = document.getElementById('mobile-number').value;\n\n            \/\/ Ensure they are stored in the hidden fields\n            document.querySelector('[name=\"emirates-id-field\"]').value = emiratesIdValue;\n            document.querySelector('[name=\"mobile-number-field\"]').value = mobileNumberValue;\n\n            \/\/ Store in localStorage as well\n            localStorage.setItem(\"cf7_emirates-id\", emiratesIdValue);\n            localStorage.setItem(\"cf7_mobile-number\", mobileNumberValue);\n\n   